    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.75.0",
    "husky": "^0.14.3",
    "lodash": "^4.17.5",
    "mocha": "^5.0.5",
    "nyc": "^11.6.0",
    "prettier": "^1.12.1",
    "pretty-quick": "^1.5.1",
    "sinon": "^6.0.0"
